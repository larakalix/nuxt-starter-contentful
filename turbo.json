{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": [
                ".vercel/**",
                ".nuxt/**",
                ".output/**",
                "dist/**",
                "storybook-static/**"
            ],
            "env": [
                "NUXT_PUBLIC_*",
                "NITRO_*",
                "ASTRO_*",
                "PUBLIC_*",
                "CONTENTFUL_*",
                "NODE_ENV"
            ]
        },
        "lint": {},
        "dev": {
            "cache": false,
            "persistent": true,
            "env": ["NUXT_PUBLIC_*", "ASTRO_*", "PUBLIC_*", "CONTENTFUL_*"]
        }
    }
}
