{
    "name": "@starter/content",
    "version": "0.0.1",
    "private": true,
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
    },
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        },
        "./types": {
            "import": "./src/types/index.ts",
            "types": "./src/types/index.ts"
        },
        "./services": {
            "import": "./src/contentful/services/index.ts",
            "types": "./src/contentful/services/index.ts"
        },
        "./utils": {
            "import": "./src/utils/index.ts",
            "types": "./src/utils/index.ts"
        },
        "./client": {
            "import": "./src/contentful/client.ts",
            "types": "./src/contentful/client.ts"
        },
        "./config": {
            "import": "./src/contentful/config/index.ts",
            "types": "./src/contentful/config/index.ts"
        },
        "./composables": {
            "import": "./src/composables/index.ts",
            "types": "./src/composables/index.ts"
        },
        "./contentful/astro": {
            "import": "./src/contentful/config/contentful.astro.ts",
            "types": "./src/contentful/config/contentful.astro.ts"
        },
        "./contentful/nuxt": {
            "import": "./src/contentful/config/contentful.nuxt.ts",
            "types": "./src/contentful/config/contentful.nuxt.ts"
        }
    },
    "peerDependencies": {
        "vue": "^3.5.18"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "eslint-config": "workspace:*",
        "tsconfig": "workspace:*",
        "tsup": "^8.0.0",
        "typescript": "~5.9.3",
        "vue": "^3.5.18"
    },
    "dependencies": {
        "@starter/domain": "workspace:*",
        "@apollo/client": "^4.0.10",
        "graphql": "^16.12.0",
        "js-sha256": "^0.11.1"
    }
}
